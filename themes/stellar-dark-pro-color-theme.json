    "menu.foreground": "#E6EDF3",
    "menu.selectionBackground": "#264466",
    "menu.selectionForeground": "#E6EDF3",
    "menu.separatorBackground": "#30363D",
    "menu.border": "#30363D"
  },

  "tokenColors": [
    /* COMMENTS --------------------------------------------------- */
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6A9955", "fontStyle": "italic" }
    },

    /* STRINGS ----------------------------------------------------- */
    {
      "scope": ["string"],
      "settings": { "foreground": "#CE9178" }
    },

    /* NUMBERS ----------------------------------------------------- */
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#B5CEA8" }
    },

    /* IMPORT / EXPORT (dark pink) -------------------------------- */
    {
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": { "foreground": "#C586C0" }
    },

    /* GENERAL KEYWORDS (dark blue: class, extends, new, etc.) ----- */
    {
      "scope": ["keyword", "keyword.operator", "storage.modifier", "storage.type.class"],
      "settings": { "foreground": "#569CD6" }
    },

    /* DECORATORS (@Module) — light yellow ------------------------- */
    {
      "scope": ["meta.decorator"],
      "settings": { "foreground": "#C4B67A" }
    },

    /* FUNCTION NAMES (forRoot) — light yellow --------------------- */
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#C4B67A" }
    },

    /* CLASS / MODULE IDENTIFIERS — dark green --------------------- */
    /* THIS IS THE FIX for ConfigModule, MinesModule, AppModule, etc. */
    {
      "scope": ["variable.other.object.ts", "entity.name.type.class", "entity.name.class", "entity.name.type", "support.class", "entity.name.namespace", "entity.other.inherited-class", "entity.name.type.module"],
      "settings": { "foreground": "#409576" }
    },

    /* PROPERTY KEYS (imports, providers, controllers) — light blue */
    {
      "scope": ["meta.object-literal.key", "variable.other.property", "support.type.property-name"],
      "settings": { "foreground": "#9CDCFE" }
    },

    /* BOOLEAN VALUES + class keyword + [] — dark blue -------------- */
    {
      "scope": ["constant.language.boolean", "storage.type", "meta.bracket.square", "keyword.control"],
      "settings": { "foreground": "#569CD6" }
    },

    /* INTERPOLATION ${...} — medium blue --------------------------- */
    {
      "scope": ["punctuation.definition.template-expression.begin", "punctuation.definition.template-expression.end", "meta.template.expression"],
      "settings": { "foreground": "#4E8DC6" }
    },

    /* VARIABLES ---------------------------------------------------- */
    /* IMPORTANT: Must come AFTER module/class override */
    {
      "scope": ["variable", "identifier", "variable.other.readwrite", "meta.object-literal.key.entity"],
      "settings": { "foreground": "#9CDCFE" }
    },

    /* PUNCTUATION -------------------------------------------------- */
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter", "meta.brace.round", "meta.brace.curly", "meta.bracket.square"],
      "settings": { "foreground": "#D4D4D4" }
    },

    /* ERRORS -------------------------------------------------------- */
    {
      "scope": ["invalid", "markup.deleted"],
      "settings": { "foreground": "#F44747" }
    }
  ]
}
